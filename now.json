      "use": "@now/node"
    },
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder@0.17.6",
      "config": {
        "serverFiles": [
          "assets/**",
          "components/**",
          "layouts/**",
          "middleware/**",
          "other/**",
          "pages/**",
          "plugins/**",
          "static/**",
          "store/**",
          "templates/**",
          "package.json"
        ]
      }
    }
  ],
